<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	  <persistence-unit name="temp" transaction-type="RESOURCE_LOCAL">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <class>com.angel.server.Contact</class>
        <class>com.angel.server.Material</class>
        <class>com.angel.server.Alumno</class>
        <class>com.angel.server.ObservacionAlumno</class>
        <class>com.angel.server.Matricula</class>
       
        <exclude-unlisted-classes>false</exclude-unlisted-classes>
        <properties>
           
            <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
            <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/contact"/>
            <property name="javax.persistence.jdbc.user" value="root"/>
            <property name="javax.persistence.jdbc.password" value="ddFerciniciativasp"/>
        </properties>
    </persistence-unit>
</persistence>
