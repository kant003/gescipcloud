<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:e="urn:import:com.google.gwt.editor.ui.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		Hello,

		<g:DecoratorPanel>
			<g:VerticalPanel>
				<g:Grid>
					<g:row>
						<g:customCell>
							<g:Label text="id" />
						</g:customCell>
						<g:customCell>
							<g:IntegerBox ui:field="id" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label text="nombre" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="nombre" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label text="apellidos" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="apellidos" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label text="nif" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="nif" />
						</g:customCell>
					</g:row>
				</g:Grid>
			</g:VerticalPanel>
		</g:DecoratorPanel>
		<g:HorizontalPanel>
			<g:Button ui:field="saveButton" text="salvar" />
			<g:Button text="cancelar" ui:field="cancelButton" />
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 